<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".startscreen.AppStartFragment">


    <Button
        android:id="@+id/app_navigations"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="App Navigation Sample" />

    <Button
        android:id="@+id/coroutines_sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="coroutines sample" />

    <Button
        android:id="@+id/work_manager_sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="work manager sample" />

    <Button
        android:id="@+id/mvvm_sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MVVM sample with Retrofit" />

    <Button
        android:id="@+id/room_db_sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Room Datbase Sample" />
    <Button
        android:id="@+id/proguard_sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pro-Guard bug" />

    <Button
        android:id="@+id/firebase_sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Firebase with MVVM" />

</LinearLayout>